// create a canvas
size(400, 400);

//STEP 1 OF STORYBOARD
background(255);
fill(0);
rect(200,0,200,400);

//STEP 2
ellipse(200, 300, 200, 200);
fill(255);
stroke(255);
ellipse(200,100,200,200);
//STEP 3
ellipse(200,300,50,50);
fill(0);
ellipse(200,100,50,50);
//STEP 4
noFill();
stroke(0);
strokeWeight(5);
ellipse(200,200,400,400);
stroke(255);
strokeWeight(100);
ellipse(200,200,500,500);